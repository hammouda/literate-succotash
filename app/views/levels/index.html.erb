<p id="notice"><%= notice %></p>


<section class="content-header">
      <h1>
        Levels
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Levels</li>
      </ol>
    </section>
    <section>
 <div class="col-md-12">
          <div class="box">
            <div class="box-header">

              <div class="box-tools">
<%= link_to 'New Level', new_level_path %>

              </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body no-padding">
              <table class="table">
                <tr>
                   <th>Designation</th>
                  <th></th>
                  <th></th>
                  <th></th>
                </tr>
    <% @levels.each do |level| %>
                 <tr>
                  <td><%= level.designation %></td>
        <td><%= link_to 'Show', level %></td>
        <td><%= link_to 'Edit', edit_level_path(level) %></td>
        <td><%= link_to 'Destroy', level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
       </tr>
                 <% end %>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->

          
        </div>
</section>
